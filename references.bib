	Issn = {1530-2075},
	Location = {Miami, Florida, USA},
	Month = {April},
	Pages = {1--10},
	Title = {Distributed Asymmetric Verification in Computational Grids},
	Url = {http://www.net.t-labs.tu-berlin.de/papers/KSW-DAVCG-08.pdf},
	Year = 2008,
