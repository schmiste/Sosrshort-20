	Location = {Kolkata, India},
	Month = {January},
	Pages = {195-206},
	Publisher = {Springer},
	Series = {Lecture Notes in Computer Science (LNCS)},
	Title = {Poisoning the Kad Network},
	Url = {http://www.net.t-labs.tu-berlin.de/papers/LMSW-PKN-10.pdf},
	Volume = 5935,
	Year = 2010,
	Bdsk-Url-1 = {http://www.net.t-labs.tu-berlin.de/papers/LMSW-PKN-10.pdf},
